/*
in = 1, sel = 00, out = a = 1
in = 1, sel = 01, out = b = 1
in = 1, sel = 10, out = c = 1
in = 1, sel = 11, out = d = 1
*/

CHIP DMux4Way {
  IN in, sel[2];
  OUT a, b, c, d;
  PARTS:
  DMux( in = in, sel = sel[1], a = ab, b = cd);
  DMux( in = ab, sel = sel[0], a = a, b = b);
  DMux( in = cd, sel = sel[0], a = c, b = d);

}